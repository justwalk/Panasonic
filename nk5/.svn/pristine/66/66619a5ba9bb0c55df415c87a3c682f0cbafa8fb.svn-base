<form class="computer-form form-horizontal no-margins" method="post" accept-charset="utf-8" id="settings-computer-menu-modal">
    <div class="modal-header">
        <button type="button" class="close" title="Close" data-dismiss="modal">&times;</button>
        <h3><%_Setting %> <%_Pack%></h3>
    </div>
    <p class="errors"></p>
    
 <div class="modal-body">
        <div class="control-group">
            <label class="control-label" for="name"><%_Name%></label>
            <div class="controls">
                <input type="text" id="task_name" class="span10" name="name"/>
                <input type="hidden" id="computer" class="span10" name="computer"/>
            </div>
        </div>
        
        <div class="control-group">
            <label class="control-label" for="memo"><%_Menu%></label>
            <div class="controls">
                   <select name="default_menu" id="default_menu" class="">
            <%= menus %>
          </select>
            </div>
        </div>

         <div class="control-group">
            <label class="control-label" for="memo"><%_Pack%></label>
            <div class="controls">
                   <select name="pack" id="pack" >
           
          </select>
            </div>
        </div>
        
 
     <div class="control-group">
       <label class="control-label" for="Operation"><%_Operation%></label>
        <div class="controls" id='wrap' >
          
            <label style="width:368px;">
                  <input type="radio" class='operation' name="operation"  style="margin-bottom: 1px;" value="0" > <%_Disable%>
            </label>

             <label style="width:368px;">
                    <input type="radio" class='operation' name="operation"  style="margin-bottom: 1px;"value="1"> <%_Clear Cache%>
            </label>

            <h5> <%_Type Of Writting%></h5>
              <label style="width:368px;">
                   <input type="radio" class='operation' name="operation"  style="margin-bottom: 1px;" value="2"> <%_Format Of Cache File%>
              </label>

            <label style="width:368px;"> 
                    <input type="radio" class='operation' name="operation"  style="margin-bottom: 1px;"value="3"> <%_RAW area(Not Used Part)%>
            </label>

             <label style="width:368px;">
                  <input type="radio"  class='operation' name="operation" style="margin-bottom: 1px;" value="4"> <%_RAW area(Full Disk)%>
             </label>
           <h5> <%_Access Mode%></h5> 
            <label style="width:368px;"> 
                  <input type="radio" class='operation' name="operation"  style="margin-bottom: 1px;"value="5"> <%_ReadORWrite%>
            </label>
            <label style="width:368px;">
                  <input type="radio"  class='operation' name="operation" style="margin-bottom: 1px;" value="6"> <%_Write Only%>
             </label>
           <label style="width:368px;">
                  <input type="radio"  class='operation' name="operation" style="margin-bottom: 1px;" value="7"> <%_Read Only%>
           </label>
      </div>
      </div>
    </div>
    
    <div class="modal-footer">
      <a href="#" class="btn" title="Close" data-dismiss="modal"><%_Cancel%></a>
      <input type="button" value="<%_Save%>" class="btn btn-primary">
    </div>
</form>