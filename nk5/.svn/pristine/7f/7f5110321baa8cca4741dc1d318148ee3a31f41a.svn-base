    <!--modal  -->
          <div class="modal fade" id="add_modal">
           <form id="add-user-form" class="form-horizontal no-margins edit-server-form" action="assaa/sacccc">
    <div class="modal-header">
        <button type="button" class="close" title="Close" data-dismiss="modal">&times;</button>
        <h3><%_Add%></h3>
    </div>
    <p class="errors"></p>
    
    <div class="modal-body">
        <div class="control-group">
          <label class="control-label" for="ip"><%_UserName%></label>
            <div class="controls">
                <input type="text"   name="username"/>
            </div>
        </div>

        <div class="control-group">
          <label class="control-label" for="port"><%_PassWord%></label>
            <div class="controls">
                <input type="password"   name="password"/>
            </div>
        </div>

        <div class="control-group">
          <label class="control-label" for="type"><%_Confim PassWord%></label>
            <div class="controls">
                 <input type="password"   name="repassword"/>
            </div>
        </div>  
   
        <div class="control-group">
          <label class="control-label" for="type"><%_name%></label>
            <div class="controls">
                 <input type="text"  name="name"/>
            </div>
        </div> 
        
    </div>
    
    <div class="modal-footer">
      <a href="#" class="btn" title="Close" data-dismiss="modal"><%_Close%></a>
      <input type="button" value="<%_Save%>" class="submit btn btn-primary" class="span12">
    </div>
</form>

         </div><!-- /.modal-content -->

